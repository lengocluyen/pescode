        <Image x:Name="imgEliminate60Points" Source="BeeImgs/Eliminate60Points.png" Canvas.Top="61" Height="29" Width="71" Canvas.Left="60" Visibility="Collapsed" RenderTransformOrigin="0.5,0.5">
        	<Image.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
