        	</Image.RenderTransform>
        </Image>
        <Image x:Name="imgEliminate100Points" Source="BeeImgs/Eliminate100Points.png" Canvas.Left="63" Canvas.Top="60" Height="29" Width="71" Visibility="Collapsed" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
