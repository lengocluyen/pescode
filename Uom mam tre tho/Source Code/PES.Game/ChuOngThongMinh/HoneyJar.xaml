        			<TranslateTransform/>
        		</TransformGroup>
        	</Image.RenderTransform></Image>
        <Image x:Name="imgVioletHexagonal" Source="BeeImgs/VioletHexagonal.png" Height="30" Width="29" Canvas.Left="131" Canvas.Top="56" Visibility="Collapsed" RenderTransformOrigin="0.5,0.5">
        	<Image.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
