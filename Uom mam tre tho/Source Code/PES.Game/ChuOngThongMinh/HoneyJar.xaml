                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="imgGreenHexagonal" Source="BeeImgs/GreenHexagonal.png" Height="26" Width="29" Canvas.Left="84" Canvas.Top="72" Visibility="Collapsed" RenderTransformOrigin="0.5,0.5">
        	<Image.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
        			<SkewTransform/>
