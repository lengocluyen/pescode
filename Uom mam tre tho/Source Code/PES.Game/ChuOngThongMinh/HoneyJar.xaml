        			<TranslateTransform/>
        		</TransformGroup>
        	</Image.RenderTransform>
        </Image>
        <TextBlock x:Name="tblMath" Canvas.Left="56" Canvas.Top="97" FontSize="16" FontFamily="Fonts/VNVOGU.TTF#.VnVogue" Foreground="Purple" Text="200 + 100" Width="87" TextAlignment="Center" />        
        <Image x:Name="imgAdd100Points" Source="BeeImgs/Add100Points.png" Canvas.Left="63" Canvas.Top="60" Height="29" Width="71" Visibility="Collapsed" RenderTransformOrigin="0.5,0.5">
