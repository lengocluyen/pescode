            </Image.RenderTransform>
        </Image>
        <Image x:Name="imgEliminate200Points" Source="BeeImgs/Eliminate200Points.png" Canvas.Left="62" Height="29" Width="81" Canvas.Top="60" Visibility="Collapsed" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
