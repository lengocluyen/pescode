        		</TransformGroup>
        	</Image.RenderTransform></Image>
        <Image x:Name="imgOrangeHexagonal" Source="BeeImgs/OrangeHexagonal.png" Height="26" Width="29" Canvas.Left="43" Canvas.Top="56" Visibility="Collapsed" RenderTransformOrigin="0.5,0.5">
        	<Image.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
        			<SkewTransform/>
